[client]
# In isolated environment sockets are pretty useless...
host = 127.0.0.1
port = 4416
protocol = tcp

[mysqld_safe]
nice = 0
socket = /tmp/mysqld.sock

[mysqld]
pid-file = /tmp/mysqld.pid
datadir	= /storage
bind-address = 127.0.0.1
port = 4416
character-set-server = utf8
collation-server = utf8_general_ci

[mysqladmin]
user = root
host = 127.0.0.1
port = 4416
protocol = tcp
